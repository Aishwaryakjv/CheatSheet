Find duplicates for sorted array:

//in java
import java.util.*;
class Main{
    public static void main(String[] args) {
        int arr[]=new int[5];
        int temp[]=new int[5];
        Scanner sc=new Scanner(System.in);
        for(int i=0;i<5;i++)
        {
            arr[i]=sc.nextInt();
        }
        int j=0;
        for(int i=0;i<4;i++)
        {
            if(arr[i]!=arr[i+1])
            temp[j++]=arr[i];
        }
        temp[j]=arr[4];
        System.out.print("\n");
        for(int i=0;i<=j;i++)
        {
            System.out.print(temp[i]+" ");
        }
    }
}
