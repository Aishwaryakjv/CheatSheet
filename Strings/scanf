#include <stdio.h>
#include <string.h>
int main()
{
    char str[10];
    scanf("%s",&str);
    for(int i=0;i<strlen(str);i++){
       printf("%c",str[i]);
    }
    
    return 0;
}

output:
abc def
abc

#include <stdio.h>
#include <string.h>
int main()
{
    char str[10];
    scanf("%[^\n]s",&str);
    for(int i=0;i<strlen(str);i++){
       printf("%c",str[i]);
    }
    
    return 0;
}

output:
abc def
abc def
